{
  "name": "server",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "editor-serve": "cd ../editor && gulp serve",
    "editor-dev": "cd ../editor && gulp dev && node clientapp.js",
    "server": "cd ../server && node app.js",
    "dev": "(start npm run editor-serve) && (start npm run server) && (start http://localhost:8000)",
    "prod": "(start npm run editor-dev) && (start npm run server)",
    "start": "npm run dev",
    "publish-test": "cp -rR ./convocode/anonymous/drafts/unit-tests ./convocode/anonymous/fsms/",
    "test": "jasmine",
    "doc": "jsdoc ./ -r -c ./jsdoc-config.json  -d ./outdocs -t ./ink-docstrap/template -R ./README.md",
    "publish-doc": "cp -rR ./outdocs ../client/b3/build/",
    "nlu": "python -m rasa_nlu.server -c ./convocode/nlu-models/rasa/config_spacy.json",
    "install-nlu": "pip install rasa_nlu;pip install rasa_nlu[spacy];python -m spacy download en_core_web_md;python -m spacy link en_core_web_md en"
  },
  "dependencies": {
    "@google-cloud/speech": "^0.9.3",
    "basic-auth": "^1.0.4",
    "basic-auth-connect": "^1.0.0",
    "bluebird": "^3.5.0",
    "body-parser": "~1.13.2",
    "chrono": "^1.0.5",
    "chrono-node": "^1.2.5",
    "clone": "^2.1.1",
    "clone-deep": "^4.0.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.7.1",
    "debug": "~2.2.0",
    "es6-map": "^0.1.5",
    "express": "^4.16.4",
    "findit": "^2.0.0",
    "fs-extra": "^2.1.2",
    "fuzzyset.js": "0.0.1",
    "glob": "^7.1.2",
    "glob-fs": "^0.1.7",
    "graceful-fs": "^4.1.11",
    "greenlock-express": "^2.0.11",
    "html-metadata": "^1.6.0",
    "ink-docstrap": "^1.3.2",
    "jade": "~1.11.0",
    "jsdoc": "^3.5.5",
    "lokijs": "^1.5.0",
    "md5": "^2.2.1",
    "moment": "~2.19.3",
    "mongojs": "^2.4.1",
    "morgan": "~1.6.1",
    "node-cache": "^3.2.1",
    "node-wit": "^4.3.0",
    "promise": "^7.1.1",
    "pubnub": "^4.12.0",
    "rxjs": "^5.5.6",
    "serve-favicon": "~2.3.0",
    "twilio": "^2.11.1",
    "uuid": "^2.0.1",
    "webshot": "^0.18.0",
    "wit": "^1.3.4",
    "xml2js": "^0.4.19"
  },
  "description": "Servo server",
  "main": "app.js",
  "devDependencies": {
    "@types/node": "^8.0.9",
    "alexa-verifier": "^0.3.6",
    "app-module-path": "^2.2.0",
    "jasmine": "^2.8.0",
    "stack-trace": "0.0.9",
    "websocket": "^1.0.24"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/servo-ai/servo-platform.git"
  },
  "keywords": [
    "chat",
    "bots"
  ],
  "author": "Lior Messinger",
  "license": "AGPL",
  "homepage": "https://github.com/servo-ai/servo-platform#readme"
}
